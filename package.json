{
	"name": "@tauri-apps/tauri-forage",
	"version": "0.0.1",
	"private": false,
	"description": "enhanced localForage with curry and crypto - for any browser and device",
	"homepage": "HTTPS://GitHub.Com/CodeEditorLand/TauriForage#readme",
	"bugs": {
		"url": "HTTPS://GitHub.Com/CodeEditorLand/TauriForage/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+HTTPS://github.com/CodeEditorLand/TauriForage.git"
	},
	"license": "SEE LICENSE IN LICENSE",
	"author": {
		"name": "Land",
		"email": "Land@Playform.Cloud",
		"url": "HTTPS://Land.Playform.Cloud"
	},
	"type": "module",
	"main": "dist/index.cjs.min.js",
	"module": "dist/index.esm.js",
	"types": "dist/index.d.ts",
	"directories": {
		"test": "test",
		"dist": "dist",
		"src": "src"
	},
	"files": [
		"dist"
	],
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"buildRollup": "rollup -c",
		"buildWebpack": "webpack --progress",
		"makeDocs": "jsdoc -c './jsdoc.json' -d docs && git add './docs/*'",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"test:jest": "jest --updateSnapshot",
		"test:unit:coverage": "jest --coverage",
		"test:unit:watch": "jest --watch",
		"test:unit:watchAll": "jest --watchAll",
		"transpile": "npx babel ./lib --out-dir ./dist && npx eslint ./dist/*.js ./dist/*.js --fix"
	},
	"lint-staged": {
		"src/*.{js,vue}": [
			"eslint --fix",
			"git add"
		]
	},
	"dependencies": {
		"localforage": "^1.7.3",
		"ramda": "^0.26.1",
		"tweetnacl": "^1.0.1",
		"tweetnacl-util": "^0.15.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.5",
		"@playform/document": "0.0.6",
		"@types/ramda": "^0.26.39",
		"better-docs": "^1.4.7",
		"dom-storage": "^2.1.0",
		"dotenv": "8.2.0",
		"jsdoc": "^3.6.3",
		"jsdoc-mermaid": "^1.0.0",
		"lint-staged": "^9.4.2",
		"postinstall-postinstall": "^2.0.0",
		"promise": "^8.0.2",
		"ts-loader": "^6.2.1",
		"tslib": "^1.10.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
